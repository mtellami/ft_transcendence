generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
	id				Int		  @default(autoincrement())
	username 	String 	@unique
	avatar 		String 
	friends		Int[]
	blocks		Int[]
	games	 		Int?	 @default(0)
	wins 	 		Int?   @default(0)
	score 		Int?   @default(0)
	online 		Boolean @default(false)
}
